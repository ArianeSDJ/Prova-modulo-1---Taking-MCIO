//Questão 01. Faça um algoritmo que receba dois números e ao final mostre a soma, subtração, divisão e multiplicação dos números lidos.


programa {
  

  funcao inicio(){
      real a
      real b
    
    escreva("Digite o primeiro número: ")
    leia(a)

    escreva("Digite o segundo número: ")
    leia(b)

    escreva("\n A soma é: ",a+b)

    escreva("\n A subtração é: ",a-b)

    escreva("\n A multiplicação é: ",a*b)

    escreva("\n A divisão é: ",a/b)
  }
}
